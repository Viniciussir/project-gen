generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id        Int      @id @default(autoincrement())
  name      String
  description String
  price     Float
  quantity  Int
  img       Bytes?   // Campo para armazenar a imagem em bin√°rio
}
